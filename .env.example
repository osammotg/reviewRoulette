# Postgres connection — use pooled URL for runtime (Neon/Supabase pooler)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DATABASE?sslmode=require"

# Direct URL for Prisma migrations (bypasses connection pooler)
DIRECT_URL="postgresql://USER:PASSWORD@HOST:5432/DATABASE?sslmode=require"

# Admin session encryption secret — generate with: openssl rand -base64 32
ADMIN_SESSION_SECRET="change-me-to-a-random-32-char-secret-before-deploy"

# Base URL for QR code generation
NEXT_PUBLIC_APP_URL="https://your-app.vercel.app"
